        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "2amigos/qrcode-library": "^1.0",
        "alx/xlswebapi": "^1.8.0",
        "andreyryabin/sprint.migration": "^3.15",
        "dompdf/dompdf": "0.8.5",
        "gregwar/captcha": "1.*",
        "hflabs/dadata": "^20.7",
        "laminas/laminas-json": "^3.1",
        "laminas/laminas-serializer": "^2.9",
        "latte/latte": "^2.9",
